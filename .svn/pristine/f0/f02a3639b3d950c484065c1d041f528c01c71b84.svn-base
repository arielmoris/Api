package pt.api.player.info;

import pt.api.bean.RequestBase;

public class PlayerInfoRequest extends RequestBase{
	
	private String playername;
	private Integer with3RDPData;
	
	public String getPlayername() {
		return playername;
	}
	public void setPlayername(String playername) {
		this.playername = playername;
	}
	public Integer getWith3RDPData() {
		return with3RDPData;
	}
	public void setWith3RDPData(Integer with3rdpData) {
		with3RDPData = with3rdpData;
	}
	
}
